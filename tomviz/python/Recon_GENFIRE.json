{
  "name" : "ReconstructGENFIRE",
  "label" : "GENFIRE Reconstruction",
  "description" : "Reconstruct a tilt series using GENeralized Fourier Iterative REconstruction (GENFIRE).",
  "children": [
    {
      "name": "reconstruction",
      "label": "Reconstruction",
      "type": "reconstruction"
    }
  ],
  "parameters" : [
    {
      "name" : "numIterations",
      "label" : "Number of Iterations",
      "type" : "int",
      "default" : 50,
      "minimum" : 1
    },
    {
      "name" : "oversamplingRatio",
      "label" : "Oversampling Ratio",
      "type" : "int",
      "default" : 3,
      "minimum" : 1
    },
    {
      "name" : "interpolationCutoffDistance",
      "label" : "Interpolation Cutoff Distance (pixels)",
      "type" : "double",
      "default" : 0.5,
      "minimum" : 0,
      "precision": 4
    },
    {
      "name" : "griddingMethod",
      "label" : "Gridding Method",
      "type" : "enumeration",
      "default" : 0,
      "options" : [
        {"FFT": 0},
        {"DFT": 1}
      ]
    },
    {
      "name" : "resolutionExtensionSuppressionState",
      "label" : "Resolution Extension/Suppression",
      "type" : "enumeration",
      "default" : 1,
      "options" : [
        {"On": 1},
        {"Off": 2},
        {"Extension Only": 3}
      ]
    },
    {
      "name" : "support",
      "label" : "Support Filename",
      "type" : "int",
      "default" : 1
    }, 
    {
      "name" : "calculateRfree",
      "label" : "Calculate R-free",
      "type" : "bool",
      "default" : true
    },
    {
      "name" : "constraintPositivity",
      "label" : "Positivity Constraint",
      "type" : "bool",
      "default" : true
    },
    {
      "name" : "constraintSupport",
      "label" : "Support Constraint",
      "type" : "bool",
      "default" : true
    },
    {
      "name" : "enforceResolutionCircle",
      "label" : "Enforce Resolution Circle",
      "type" : "bool",
      "default" : true
    },
    {
      "name" : "permitMultipleGridding",
      "label" : "Allow Multiple Gridding",
      "type" : "bool",
      "default" : false
    },
    {
      "name" : "verbose",
      "label" : "Verbose Mode",
      "type" : "bool",
      "default" : true
    }
  ]
}
